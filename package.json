{
  "name": "@rx-signals/store",
  "version": "1.0.0-rc.3",
  "description": "Reactive effects management with bahaviors and event streams",
  "author": "Gerd Neudert",
  "license": "MIT",
  "main": "dist/cjs/store.js",
  "module": "dist/esm/store.js",
  "types": "dist/esm/store.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/gneu77/rx-signals"
  },
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "build": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "test": "jest --config jestconfig.json",
    "lint": "eslint --ext .ts \"src/**\"",
    "pre-publish": "rm -rf dist && npm run format && npm run lint && npm run test && npm run build"
  },
  "keywords": [
    "rxjs",
    "reactive programming",
    "behaviors",
    "events",
    "signals",
    "streams",
    "store",
    "effects management",
    "state management",
    "typescript"
  ],
  "files": [
    "dist",
    "src",
    "CHANGELOG.md",
    "LICENSE",
    "package.json",
    "README.md",
    "tsconfig.json"
  ],
  "dependencies": {
    "rxjs": "^6.6.3"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "eslint": "^7.10.0",
    "eslint-config-airbnb-typescript": "^11.0.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.5.0",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3"
  }
}
